{
  "manifest_version": 3,
  "name": "Noodlr Walkthrough",
  "version": "0.1.0",
  "description": "Adds an interactive walkthrough guide for noodlr.ai",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["https://app.noodlr.ai/*"],
    "css": ["assets/main.css"],
    "js": ["assets/main.js"]
  }],
  "web_accessible_resources": [{
    "resources": ["assets/*"],
    "matches": ["https://app.noodlr.ai/*"]
  }]
}