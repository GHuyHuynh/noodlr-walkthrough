{
  "manifest_version": 3,
  "name": "Noodlr Walkthrough",
  "version": "0.1.0",
  "description": "Adds an interactive walkthrough guide for noodlr.ai",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["https://app.noodlr.ai/*"],
    "css": ["styles.css"],
    "js": ["walkthrough.js", "content.js"]
  }]
}